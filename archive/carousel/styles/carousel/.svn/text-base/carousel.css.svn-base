    background: transparent url(../../graphics/carousel/icon_carousel_nextHvr.png) no-repeat 0 0;
 _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="../../graphics/carousel/icon_carousel_nextHvr.png", sizingMethod="crop");
