}
.esriPopup.myTheme .contentPane {
    border-bottom: none;
}
.esriPopup.myTheme .titlePane, .dj_ie7 .esriPopup.myTheme .titlePane .title {
    background-color: #114562;
    color: #fff;
    font-weight: bold;
    border:none;
}
.esriPopup.myTheme a {
    color: #19628c;
}
